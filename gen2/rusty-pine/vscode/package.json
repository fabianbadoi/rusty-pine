{
  "name": "rusty-pine-vscode-lsp",
  "version": "1.0.0",
  "description": "Rusty Pine LSP extension for VS Code",
  "main": "src/extension.js",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onLanguage:pine"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pine",
        "aliases": [
          "Pine"
        ],
        "extensions": [
          ".pine"
        ],
        "configuration": "./pine.configuration.json"
      }
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/vscode": "^1.79.1",
    "path": "^0.12.7",
    "vscode-jsonrpc": "^8.1.0",
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver-textdocument": "^1.0.8"
  }
}
